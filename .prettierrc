{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "assets/",
    "test/",
    "configs/",
    "routes/",
    "api/",
    "store/",
    "contexts/",
    "hooks/",
    "types/",
    "interfaces/",
    "constants/",
    "helpers/",
    "utils/",
    "components/",
    "styles/",
    "^./(.(?!.*.css$))",
    "^../(.*)",
    ".css"
  ],
  "importOrderSortSpecifiers": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "printWidth": 120
}
